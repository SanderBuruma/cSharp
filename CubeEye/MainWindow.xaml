<Window x:Class="CubeEye.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CubeEye"
        mc:Ignorable="d"
        Title="Cube Eye v0.0.1" Height="600" Width="800" Background="#FFC0C0FF" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" WindowState="Maximized" FontFamily="Times New Roman" WindowStyle="ToolWindow">
    <Grid>
        <Button x:Name="StartButton" Content="Start!" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="92" Height="75" Background="White" Foreground="Black" FontSize="18" Cursor="None">
            <Button.BorderBrush>
                <SolidColorBrush Color="#FF003AFF" Opacity="0.3"/>
            </Button.BorderBrush>
        </Button>

    </Grid>
</Window>
